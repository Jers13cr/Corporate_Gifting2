        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading) {
        return;
    }
	if (newValue == ''){
		g_form.setValue('e_voucher_price', '');
		return;
	}
    //Calculates the E-Voucher Price when the E-Voucher amount is changed 


    var price = parseInt(newValue) * parseInt(g_form.getValue('quantity'));
    g_form.setValue('e_voucher_price', "Rs."+price);


}]]></script>
