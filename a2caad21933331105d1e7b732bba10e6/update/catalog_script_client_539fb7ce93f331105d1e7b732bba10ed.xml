        <script><![CDATA[function onLoad() {
    //Type appropriate comment here, and begin script below
    var eVoucherGA = new GlideAjax('e_voucherUtils');
    eVoucherGA.addParam('sysparm_name', 'getProjectAccount');
    eVoucherGA.addParam('sysparm_userID', g_user.userID);
    eVoucherGA.getXML(projectAccountParse);

    function projectAccountParse(response) {
        try {
            var answerFromXML = response.responseXML.documentElement.getAttribute("answer");
            g_form.setValue('project_account', answerFromXML);
        } catch (err) {
            g_form.addErrorMessage(err);
        }
    }
	



}]]></script>
